<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Suite1" verbose="1">
    <test name="LoginS001" preserve-order="false">
        <classes>
            <class name="TestLogin">
                <methods>
                    <include name="Navigation"/>
                    <include name="PositiveLogin"/>
                    <include name="NegativeLogin"/>
                    <include name="LogOut"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="RegistrationS002" preserve-order="false">
        <classes>
            <class name="TestRegisterProcess">
                <methods>
                    <include name="userRegistration"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
